@Inbook{Kroemer1985,
author="Kroemer, Herbert",
editor="Chang, Leroy L.
and Ploog, Klaus",
title="Theory of Heterojunctions: A Critical Review",
bookTitle="Molecular Beam Epitaxy and Heterostructures",
year="1985",
publisher="Springer Netherlands",
address="Dordrecht",
pages="331--379",
abstract="The central aspect of an abrupt heterojunction, and the point of departure for all its device properties, is the exact lineup of the bands of the two semiconductors at the interface. Band lineups vary over a wide range. Lineup data from numerous heterosystems have been reported in the literature, but only a few can be considered truly reliable. Of several theories that have been proposed to explain and/or predict band lineups, the Harrison Atomic Orbital (HAO) Theory has been by far the most successful; it agrees with those experimental data that are considered most trustworthy to within {\textpm}0.13 eV (standard deviation). Also reviewed are the Frensley-Kroemer Pseudopotential (FKP) theory, the Electron Affinity Rule (EAR), and Self-Consistent Interface Potential (SCIP) calculations of band lineups.",
isbn="978-94-009-5073-3",
doi="10.1007/978-94-009-5073-3_10",
url="https://doi.org/10.1007/978-94-009-5073-3_10"
}


